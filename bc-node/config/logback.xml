<configuration>

    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%d{HH:mm:ss.SSS} [%thread] %logger - %msg%n</pattern>
        </encoder>
    </appender>

    <logger name="synitex.backup" level="debug" />

    <root level="info">
        <appender-ref ref="STDOUT" />
    </root>

</configuration>